{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-undef": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "eqeqeq": "off",
        "object-curly-newline": "off",
        "consistent-return": "off"

    }
}
